{
  "name": "gpt-task-manager-obsidian",
  "version": "1.0.0",
  "description": "AI-powered task manager for Obsidian with GPT integration, voice input, and smart task suggestions",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tsc -noEmit -skipLibCheck",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "task-manager",
    "gpt",
    "ai",
    "voice-input",
    "productivity",
    "i18n",
    "accessibility"
  ],
  "author": "Eddie",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.17.3",
    "jest": "^29.5.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0"
  }
}

